{
  "version": 2,
  "routes": [
    {
      "src": "/mobile",
      "dest": "/mobile.html"
    },
    {
      "src": "/controller",
      "dest": "/controller.html"
    },
    {
      "src": "/desktop",
      "dest": "/desktop.html"
    },
    {
      "src": "/test",
      "dest": "/test.html"
    },
    {
      "src": "/debug-mobile.html",
      "dest": "/debug-mobile.html"
    },
    {
      "src": "/test-deployment.html",
      "dest": "/test-deployment.html"
    },
    {
      "src": "/api/events",
      "dest": "/api/events.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*\\.(html|css|js|png|jpg|jpeg|gif|svg|ico))",
      "dest": "/$1"
    },
    {
      "src": "/",
      "dest": "/api/index.js"
    }
  ]
}